programa {
	funcao inicio() {
	    real Minformatica[3][5] ,Mseguranca[3][5] ,Mempreendedorismo[3][5] ,Mdados[3][5] ,Mweb[3][5]
	    cadeia Mnome[3][5]
	    inteiro Mindice ,Mturma
	    Mindice=0

    para (Mturma = 0; Mturma <= 2; Mturma ++)
    {
        para (Mindice = 0; Mindice <= 4; Mindice ++)
        {
            escreva ("Digite um nome: ")
            leia (Mnome[Mturma][Mindice])
            escreva ("Digite uma nota de informatica: ")
            leia (Minformatica[Mturma][Mindice])
            escreva ("Digite uma nota de seguranÃ§a: ")
            leia (Mseguranca[Mturma][Mindice])
            escreva ("Digite uma nota de empreendedorismo: ")
            leia (Mempreendedorismo[Mturma][Mindice])
            escreva ("Digite uma nota de dados: ")
            leia (Mdados[Mturma][Mindice])
            escreva ("Digite uma nota de web: ")
            leia (Mweb[Mturma][Mindice])
        }
    }    
    para (Mturma = 0; Mturma <= 2; Mturma ++)
    {
        escreva ("\nTurma - ",Mturma, "\nDiciplina informatica: ")
        para (Mindice=0;Mindice <= 4; Mindice++)
        {
            escreva ("\n- " ,Mnome[Mturma][Mindice])
            se (Minformatica[Mturma][Mindice]>=60)
            escreva(" APROVADO") 
            senao
            escreva (" REPROVADO")
        }
        escreva ("\nDisciplina Seguranca:")
            para (Mindice= 0; Mindice<= 4 ; Mindice++)
        {
        escreva ("\n- " ,Mnome[Mturma][Mindice])
            se (Mseguranca[Mturma][Mindice]>=60)
            escreva(" Aprovado")
            senao
            escreva(" Reprovado")
        }
        escreva("\nDiciplina Empreendedorismo:")
        para (Mindice=0;Mindice <= 4; Mindice++)
        {
            escreva("\n-" ,Mnome[Mturma][Mindice])
            se (Mempreendedorismo[Mturma][Mindice]>=60)
            escreva(" Aprovado")
            senao
            escreva(" Reprovado")
        }
        escreva("\nDiciplina Dados:")
        para (Mindice=0;Mindice <= 4; Mindice++)
        {
            escreva("\n- " ,Mnome[Mturma][Mindice])
            se (Mdados[Mturma][Mindice]>=60)
            escreva(" Aprovado")
            senao
            escreva(" Reprovado")
        }
         escreva("\nDiciplina Web:")
        para (Mindice=0;Mindice <= 4; Mindice++)
        {
            escreva("\n- " ,Mnome[Mturma][Mindice])
            se (Mweb[Mturma][Mindice]>=60)
            escreva(" Aprovado")
            senao
            escreva(" Reprovado")
        }
    }
    }
}
